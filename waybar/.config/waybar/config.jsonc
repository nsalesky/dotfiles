{
    "layer": "top", // Waybar at top layer
    "position": "bottom", // Waybar position (top|bottom|left|right)
    "mod": "dock",
    "height": 40, // Waybar height (to be removed for auto height)
    "exclusive": true,
    "passthrough": false,
    "gtk-layer-shell": true,
    // "width": 1280, // Waybar width
    // Choose the order of the modules
    "modules-left": ["cpu", "memory", "clock"],
    "modules-center": ["wlr/workspaces", "hyprland/window"],
    "modules-right": ["custom/updates", "network", "bluetooth", "wireplumber", "idle_inhibitor", "custom/wallchange", "custom/mode", "tray", "battery"],

    // Center modules
    "wlr/workspaces": {
        "disable-scroll": true,
        "all-outputs": true,
        "on-click": "activate",
        
        "format": "{name}: {icon}",
        "format-icons": {
            "1": "ÔÑ†  ",
            "2": "Û∞àπ  ",
            "3": "ÔÑ°  ",
            "4": "ÔÜº  ",
            "5": "ÔÅµ  ",
            "urgent": "ÔÅ™  ",
            "focused": "ÔÜí  ",
            "default": "ÔÑë  "
        },

        // "persistent-workspaces": {
