        "rewrite": {
            "(.*) ‚Äî Mozilla Firefox": "$1 Û∞àπ",
            "(.*)Mozilla Firefox": "Firefox Û∞àπ",
            "(.*)Spotify": "Spotify Û∞ìá",
            "(.*)Steam": "Steam Û∞ìì"
        }
    },

    // left modules
    "cpu": {
        "interval": 10,
        "format": "Û∞çõ {usage}%",
        "format-alt": "{icon0}{icon1}{icon2}{icon3}",
        "format-icons": ["‚ñÅ", "‚ñÇ", "‚ñÉ", "‚ñÑ", "‚ñÖ", "‚ñÜ", "‚ñá", "‚ñà"]
    },

    "memory": {
        "interval": 30,
        "format": "Û∞æÜ {percentage}%",
        "format-alt": "Û∞æÖ {used}GB",
        "max-length": 10,
        "tooltip": true,
        "tooltip-format": "Óâ¶ {used:0.1f}GB/{total:0.1f}GB"
    },

    "clock": {
        "format": "{:Ôê∫ %I:%M %p Û∞É≠ %a %d}",
        "format-alt": "{:Û∞•î %H:%M ÔÑ≥ %b %Y}",
        "tooltip-format": "<tt><big>{calendar}</big></tt>"
    },

    // right modules
    "idle_inhibitor": {
        "format": "{icon}",
        "format-icons": {
            "activated": "ÔÅÆ",
            "deactivated": "ÔÅ∞"
        }
    },

    "custom/updates": {
        "format": "Û∞ÆØ {}",
        "exec": "~/.config/waybar/scripts/update-sys.sh",
        "on-click": "~/.config/waybar/scripts/update-sys.sh update",
        "interval": 86400, // once every day
        "tooltip": true
    },
    
    "network": {
        // "interface": "wlp2*", // (Optional) To force the use of this interface
        "format-wifi": "Û∞§® {essid}",
        "format-ethernet": "Û±òñ Wired",
        "tooltip-format": "Û±òñ {ipaddr} ÔÑπ {bandwidthUpBytes} ÔÑ∫ {bandwidthDownBytes}",
        "format-linked": "Û±òñ {ifname} (No IP)",
        "format-disconnected": "ÔÅ± Disconnected",
        "format-alt": "Û∞§® {signalStrength}%",
        "interval": 5
    },

    "bluetooth": {
	    "format": "",
	    "format-disabled": "", // an empty format will hide the module
	    "format-connected": "Ôäì {num_connections}",
	    "tooltip-format": "Ôäì {device_alias}",
	    "tooltip-format-connected": "{device_enumerate}",
	    "tooltip-format-enumerate-connected": "Ôäì {device_alias}"
    },

    "wireplumber": {
        "format": "{volume}% {icon}",
        "format-muted": "ÔÄ¶",
        "on-click": "helvum",
        "format-icons": ["ÔÄ¶", "ÔÄß", "ÔÄ®"]
    },

    "custom/wallchange": {
        "format": "Ôûü{}",
        "exec": "~/.config/swww/wallpaper-control.sh -t",
        "on-click": "~/.config/swww/wallpaper-control.sh -n",
        "interval" : 86400, // once every day
        "tooltip": true
    },

    "custom/mode": {
        "format": "Óäù{}",
        "exec": "echo ; echo Û∞ü° switch mode",
        "on-click": "~/.config/hypr/mode-switch.sh switch",
        "interval" : 86400, // once every day
        "tooltip": true
    },

    "tray": {
        "icon-size": 15,
        "spacing": 5
    },

    "battery": {
        "states": {
            // "good": 95,
            "warning": 30,
            "critical": 15
        },
        "format": "{capacity}% {icon}",
        "format-charging": "{capacity}% Ôóß",
        "format-plugged": "{capacity}% Ôá¶",
        "format-alt": "{time} {icon}",
        // "format-good": "", // An empty format will hide the module
        // "format-full": "",
        "format-icons": ["ÔâÑ", "ÔâÉ", "ÔâÇ", "ÔâÅ", "ÔâÄ"]
    },
}

